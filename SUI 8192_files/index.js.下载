const {
  originalMainnetContractAddress,
  mainnetContractAddress,
  mainnetLeaderboardAddress,
  mainnetMaintainerAddress,
  testnetContractAddress,
  testnetLeaderboardAddress,
  testnetMaintainerAddress,
  devnetContractAddress,
