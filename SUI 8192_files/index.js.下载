      status = EthosConnectStatus.Connected;
    } else if (extensionState.isConnecting) {
      status = EthosConnectStatus.Loading;
    } else if (provider && extensionState.isConnected) {
      status = EthosConnectStatus.Connected;
    } else {
      status = EthosConnectStatus.NoConnection;
    }
