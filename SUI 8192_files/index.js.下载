  constraints: SuiMoveAbilitySet,
  isPhantom: (0, import_superstruct7.boolean)()
});
var SuiMoveNormalizedTypeParameterType = (0, import_superstruct7.object)({
  TypeParameter: (0, import_superstruct7.number)()
});
var MoveCallMetric = (0, import_superstruct7.tuple)([
  (0, import_superstruct7.object)({
    module: (0, import_superstruct7.string)(),
    package: (0, import_superstruct7.string)(),
    function: (0, import_superstruct7.string)()
  }),
  (0, import_superstruct7.string)()
]);
